{
    "name": "basic/laravel-eloquent-repository",
    "description": "Database repository for laravel 5.8 and up",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "lumen",
        "database",
        "orm",
        "eloquent"
    ],
    "authors": [
        {
            "name": "tri-suli",
            "email": "trisulipras@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Basic\\DatabaseRepository\\": "src/Basic"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Basic\\DatabaseRepository\\Test\\": "tests"
        }
    },
    "require": {
        "php" : "^7.2.5|^8.0",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev"
}
